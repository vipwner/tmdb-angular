  "angular-cli": {},
  "scripts": {
    "start": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng serve --port 8003 --aot",
    "build": "npm run test && npm run lint && ng build --prod --aot",
    "test": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng test tmdb-angular --watch=false --code-coverage",
    "test-watch": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng test --watch=true ",
    "test-focus-checks": "! grep -Hrn --include='*.spec.ts' 'fdescribe\\|fit' src",
    "coverage": "./node_modules/http-server/bin/http-server -c-1 -o -p 9875 ./coverage",
    "test-coverage": "ng test --watch=false --code-coverage && ./node_modules/http-server/bin/http-server -c-1 -o -p 9875 ./coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ngh",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng"
  },
  "private": true,
